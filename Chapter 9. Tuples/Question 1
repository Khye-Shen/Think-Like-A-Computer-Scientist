Names = ("Julia", "Roberts", 1967, "Duplicity", 2009, "Actress", "Atlanta, Georgia")


def tt(a):
    c = 2 * math.pi * a
    b = math.pi * a * a
    return (c, b)
    
    
print(Names[2:])
print(Names[3:])
print(Names[4:])
print(Names[:2])
print(Names[:3])
print(Names[:4])
print(Names[5:])
